{
  "name": "@lykmapipo/cap-consumer",
  "version": "0.1.16",
  "description": "Common Alert Protocol consumer",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "clean": "rimraf lib dist es umd logs",
    "prebuild": "npm test && npm run clean",
    "build": "rollup -c",
    "lint": "eslint --fix --ext .js src/ test/ rollup.config.js",
    "pretest": "npm run lint",
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text mocha --exit --timeout=8000 --require @babel/register test/**/*.spec.js",
    "test:unit": "NODE_ENV=test npm run pretest && NODE_ENV=test mocha --exit --timeout=8000 --require @babel/register test/unit/**/*.spec.js",
    "test:integration": "NODE_ENV=test npm run pretest && NODE_ENV=test mocha --exit --timeout=8000 --require @babel/register test/integration/**/*.spec.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "docs": "doxdox 'lib/**/*.js' -p package.json -l markdown -o DOCUMENTATION.md",
    "cmt": "git add -A && git-cz",
    "changelog": "changelog",
    "chore:deps": "ncu -u && npm install --save",
    "lint:config": "eslint --print-config rollup.config.js"
  },
  "keywords": [
    "lykmapipo",
    "client",
    "request",
    "common",
    "alert",
    "protocol",
    "cap",
    "consumer",
    "feed"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lykmapipo/cap-consumer.git"
  },
  "author": {
    "name": "lykmapipo",
    "email": "lallyelias87@gmail.com",
    "url": "https://github.com/lykmapipo"
  },
  "contributors": [
    {
      "name": "lykmapipo",
      "email": "lallyelias87@gmail.com",
      "url": "https://github.com/lykmapipo"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lykmapipo/cap-consumer/issues"
  },
  "homepage": "https://github.com/lykmapipo/cap-consumer#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.4",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@commitlint/travis-cli": "^9.0.1",
    "@lykmapipo/test-helpers": "^0.5.25",
    "chai": "^4.2.0",
    "commitizen": "^4.1.2",
    "coveralls": "^3.1.0",
    "cz-conventional-changelog": "^3.2.0",
    "doxdox": "^3.0.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^28.6.0",
    "eslint-plugin-mocha": "^7.0.1",
    "eslint-plugin-prettier": "^3.1.4",
    "generate-changelog": "^1.8.0",
    "handlebars": "^4.7.6",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "lodash.template": "^4.5.0",
    "mocha": "^8.0.1",
    "nock": "^13.0.1",
    "npm-check-updates": "^7.0.1",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.18.1",
    "shelljs": "^0.8.4"
  },
  "peerDependencies": {},
  "dependencies": {
    "@lykmapipo/common": ">=0.37.1",
    "@lykmapipo/env": ">=0.17.11",
    "@lykmapipo/geo-tools": ">=0.7.8",
    "@lykmapipo/http-client": ">=0.4.11",
    "async": ">=3.2.0",
    "feedparser": ">=2.2.10",
    "lodash": ">=4.17.15",
    "xml2js": ">=0.4.23"
  },
  "engines": {
    "node": ">=12.4.0",
    "npm": ">=6.9.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint",
      "git add -A"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
